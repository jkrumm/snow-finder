{
  "tasks": {
    "dev": "deno run -A --node-modules-dir=auto npm:vite",
    "build": "deno run -A --node-modules-dir=auto npm:vite build",
    "server:start": "deno run -A --node-modules-dir --watch ./server/main.ts",
    "serve": "deno task build && deno task server:start"
  },
  "imports": {
    "@blueprintjs/select": "npm:@blueprintjs/select@^5.3.5",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@oak/oak": "jsr:@oak/oak@^17.1.3",
    "@preact/signals-react": "npm:@preact/signals-react@^2.2.0",
    "@preact/signals-react-transform": "npm:@preact/signals-react-transform@^0.4.0",
    "@std/assert": "jsr:@std/assert@1",
    "@tailwindcss/container-queries": "npm:@tailwindcss/container-queries@^0.1.1",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.0-beta.5",
    "@types/react": "npm:@types/react@^18.3.12",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.3",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-router-dom": "npm:react-router-dom@^6.28.0",
    "sass": "npm:sass@^1.82.0",
    "tailwindcss": "npm:tailwindcss@^4.0.0-beta.5",
    "vite": "npm:vite@^5.4.11",
    "cheerio": "npm:cheerio@^1.0.0",
    "luxon": "npm:luxon@^3.5.0",
    "@blueprintjs/core": "npm:@blueprintjs/core@^5.16.0",
    "@blueprintjs/icons": "npm:@blueprintjs/icons@^5.15.0",
    "normalize.css": "npm:normalize.css@^8.0.1"
  },
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
